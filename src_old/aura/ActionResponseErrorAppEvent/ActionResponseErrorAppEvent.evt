<aura:event type="APPLICATION" description="Action Error Response App Event">
  <aura:attribute name="id" type="String" description="event identifier" default=""/>
  <aura:attribute name="route" type="String" description="classname/methodName" default=""/>
  <aura:attribute name="error" type="String" description="action response return error" default=""/>
  <aura:attribute name="parameters" type="object" description="the parameters passed to the route"/>
</aura:event>
    <validationRules>
        <fullName>BranchingQuestionCannotBeOptional</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR(QuestionType__c == &apos;Single-Select List&apos;,QuestionType__c == &apos;Yes/No&apos; ), ISPICKVAL(JumpToAction__c, &apos;BRANCH&apos;), Optional__c )</errorConditionFormula>
        <errorMessage>Branching question cannot be optional.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MinMaxValue_BothOrNeither</fullName>
        <active>true</active>
        <errorConditionFormula>OR(AND(ISBLANK(MinValue__c),NOT(ISBLANK(MaxValue__c))),AND(NOT(ISBLANK(MinValue__c)),ISBLANK(MaxValue__c)))</errorConditionFormula>
        <errorMessage>Both min / max value should be added or neither of them should have a value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>validateOneActiveResponse</fullName>
        <active>true</active>
        <errorConditionFormula>AND ( NOT(ISNEW()), 
					DefinedResponseCount__c &lt; 2,  
					NOT(ISPICKVAL(ProjectService__r.Project__r.CloneStatus__c,&apos;Started&apos;)),
					OR(QuestionType__c == &apos;Single-Select List&apos;,QuestionType__c ==&apos;Multi-Select List&apos; ))</errorConditionFormula>
        <errorMessage>There should be at least two active Defined Responses for this question.</errorMessage>
    </validationRules>

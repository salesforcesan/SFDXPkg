        <active>true</active>
        <errorConditionFormula>AND(ISCHANGED( LocationScheduledDate__c),LocationScheduledDate__c &lt; TODAY())</errorConditionFormula>
        <errorMessage>Cannot schedule a location to a date that is in the past.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_End_Time</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(
OR(
ISBLANK(EndTime__c),
REGEX(EndTime__c, &quot;^(1[0-2]|0?[1-9]):[0-5][0-9] ([AaPp][Mm])$&quot;)
)
)</errorConditionFormula>
        <errorMessage>Invalid End Time. Please follow this format HH:MM AMPM</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Schedule_Date</fullName>
        <active>true</active>
        <description>Schedule date should be within the project window</description>
        <errorConditionFormula>AND( UPPER(Project__r.ProjectScheduleOptionCode__c) == &apos;USER&apos;,NOT(ISBLANK(LocationScheduledDate__c)), OR( Project__r.EndDate__c - LocationScheduledDate__c &lt; (Project__r.NumberOfDays__c - 1), Project__r.EndDate__c &lt; LocationScheduledDate__c, Project__r.StartDate__c &gt; LocationScheduledDate__c))</errorConditionFormula>
        <errorMessage>Schedule date should be within the project window.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Start_Time</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(
OR(
ISBLANK(StartTime__c),
REGEX(StartTime__c, &quot;^(1[0-2]|0?[1-9]):[0-5][0-9] ([AaPp][Mm])$&quot;)
)
)</errorConditionFormula>
        <errorMessage>Invalid Start Time. Please follow this format HH:MM AMPM</errorMessage>
    </validationRules>

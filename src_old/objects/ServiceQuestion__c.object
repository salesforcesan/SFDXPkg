    <validationRules>
        <fullName>BranchingQuestionCannotBeOptional</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR(QuestionType__c == &apos;Single-Select List&apos;,QuestionType__c == &apos;Yes/No&apos; ), ISPICKVAL(JumpToAction__c, &apos;BRANCH&apos;), Optional__c )</errorConditionFormula>
        <errorMessage>Branching question cannot be optional.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ExceptionQuestionCannotBeOptional</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Question__r.ExceptionQuestion__c,Optional__c)</errorConditionFormula>
        <errorMessage>Exception Question cannot be optional</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>validateOneActiveResponse</fullName>
        <active>true</active>
        <errorConditionFormula>AND ( NOT(ISNEW()), NumberofDefinedResponses__c &lt; 2, OR(QuestionType__c == &apos;Single-Select List&apos;,QuestionType__c ==&apos;Multi-Select List&apos; ))</errorConditionFormula>
        <errorMessage>There should be at least two (2) active Defined Responses for this question.</errorMessage>
    </validationRules>

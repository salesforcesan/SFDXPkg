    <validationRules>
        <fullName>Validate_PLU_or_Item_Number_not_empty</fullName>
        <active>true</active>
        <description>Ensures Item Number or UPC field are populated.</description>
        <errorConditionFormula>AND(
ISBLANK(StockCode__c),
ISBLANK(UPC__c))</errorConditionFormula>
        <errorMessage>To move forward please provide either a Stock Code or a UPC</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_UPC_Length</fullName>
        <active>true</active>
        <errorConditionFormula>LEN(UPC__c) &gt; 0 &amp;&amp; Len(UPC__c)&lt; 12</errorConditionFormula>
        <errorMessage>UPC should be 12 digits</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_package_size_Qty</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(PackageSizeQty__c)</errorConditionFormula>
        <errorMessage>Invalid Package Size Qty</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_package_size_Units</fullName>
        <active>false</active>
        <errorConditionFormula>NOT(
   OR(
      ISPICKVAL(PackageSizeUnits__c,&apos;count&apos;),
      ISPICKVAL(PackageSizeUnits__c,&apos;lb&apos;),
      ISPICKVAL(PackageSizeUnits__c,&apos;oz&apos;),
      ISPICKVAL(PackageSizeUnits__c,&apos;pint&apos;),
      ISPICKVAL(PackageSizeUnits__c,&apos;gallon&apos;),
      ISPICKVAL(PackageSizeUnits__c,&apos;quart&apos;)
 )
)</errorConditionFormula>
        <errorMessage>Invalid Package Size Units</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>validate_name</fullName>
        <active>true</active>
        <errorConditionFormula>LEN(Name)=0</errorConditionFormula>
        <errorMessage>Item Name cannot be empty.</errorMessage>
    </validationRules>

    <validationRules>
        <fullName>Disallow_Product_change_after_locations</fullName>
        <active>true</active>
        <description>Do not allow a product to be changed after adding reservation locations</description>
        <errorConditionFormula>AND (ISCHANGED( Service__c ), NumberofReservedLocations__c &gt; 0)</errorConditionFormula>
        <errorMessage>Cannot change service (product), after adding reservation locations. Remove all locations and retry.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Disallow_booked_with_0locations</fullName>
        <active>true</active>
        <description>At least one location must be reserved before a reservation can be approved/booked.</description>
        <errorConditionFormula>AND(ISCHANGED(Status__c), OR(TEXT(Status__c) == &apos;Pending Approval&apos;,TEXT(Status__c) == &apos;Approved&apos;, TEXT(Status__c) == &apos;Reserved&apos; ), NumberofReservedLocations__c == 0)</errorConditionFormula>
        <errorMessage>At least one location should be reserved before a reservation can be submitted for approval or booked.</errorMessage>
    </validationRules>

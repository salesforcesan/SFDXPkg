    <validationRules>
        <fullName>Disallow_materials_for_MultiDay_Projects</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISNEW() ,ProjectService__r.Project__r.ProjectType__r.AllowMultidays__c == True)</errorConditionFormula>
        <errorMessage>Materials cannot be added to a multi day project.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invalid_Material</fullName>
        <active>true</active>
        <errorConditionFormula>Material__c = null || Material__c = &apos;&apos;</errorConditionFormula>
        <errorMessage>Invalid Material. Please select a valid material.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invalid_Quantity</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
ISNULL(Quantity__c),
Quantity__c&lt;=0 || Quantity__c&gt;99999)</errorConditionFormula>
        <errorDisplayField>Quantity__c</errorDisplayField>
        <errorMessage>Please enter a valid quantity between 1 and 99999</errorMessage>
    </validationRules>

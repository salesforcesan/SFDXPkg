    <validationRules>
        <fullName>Project_Status_check_on_save</fullName>
        <active>true</active>
        <description>Don&apos;t allow to inactive a defined response when the project is &quot;In Progress&quot;</description>
        <errorConditionFormula>AND (ISCHANGED(Active__c ),Active__c == false,  ProjectServiceQuestion__r.ProjectService__r.ProjectStatus__c ==&apos;In Progress&apos; )</errorConditionFormula>
        <errorMessage>Defined response cannot be edited when project is In Progress</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_2_active_defined_responses</fullName>
        <active>true</active>
        <errorConditionFormula>AND (ISCHANGED(Active__c ),Active__c == false, ProjectServiceQuestion__r.DefinedResponseCount__c &lt;= 2 )</errorConditionFormula>
        <errorMessage>There should be at least two (2) active Defined Responses for this question.</errorMessage>
    </validationRules>

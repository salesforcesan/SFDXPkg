    <validationRules>
        <fullName>CanNotChangeJobIfClosed</fullName>
        <active>true</active>
        <description>A Job in a CLOSED status cannot have further actions performed.</description>
        <errorConditionFormula>LOWER(PRIORVALUE(JobStatus__c)) == &apos;closed&apos;</errorConditionFormula>
        <errorMessage>A Job in a CLOSED status cannot have further actions performed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cancel_Invalid_Project_Status</fullName>
        <active>false</active>
        <errorConditionFormula>AND(LOWER(TEXT(PRIORVALUE(JobStatus__c))) == &apos;canceled&apos;, LOWER(TEXT(Project__r.Status__c)) != &apos;booked&apos;, LOWER(TEXT(Project__r.Status__c)) != &apos;launched&apos;, LOWER(TEXT(Project__r.Status__c)) != &apos;in progress&apos;)</errorConditionFormula>
        <errorMessage>Cannot cancel job, invalid Job / Project status. Job cannot be canceled if it is already canceled or the project is not in one these statuses (Booked, Launched, In Progress).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Canceled_Reason_Other_Requires_Comments</fullName>
        <active>true</active>
        <errorConditionFormula>LOWER(TEXT(JobStatus__c)) == &apos;canceled&apos; &amp;&amp; LOWER(TEXT(JobCanceledReason__c)) == &apos;other&apos; &amp;&amp; TRIM(JobCanceledComments__c) == &apos;&apos;</errorConditionFormula>
        <errorMessage>Please provide cancel comments when cancel reason is Other.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CannotCancelReportedJob4EndedProject</fullName>
        <active>true</active>
        <description>CSM can&apos;t cancel a reported job if the project is ended.</description>
        <errorConditionFormula>AND(
LOWER(TEXT(JobStatus__c)) == &apos;canceled&apos;,
LOWER(TEXT(PRIORVALUE(JobStatus__c))) == &apos;reported&apos;,
LOWER(ProjectStatus__c) == &apos;ended&apos;
)</errorConditionFormula>
        <errorMessage>CSM can&apos;t cancel a reported job if the project is ended.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Disallow_JobSchedDateChange_byPB_ForEC</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT CONTAINS($User.Username,&quot;salesforce.integration&quot;), ISCHANGED(JobScheduledDate__c),  LOWER(ProjectScheduleOptionCode__c) = &apos;ec&apos;)</errorConditionFormula>
        <errorMessage>Project Builder cannot change the scheduled date for a &apos;Execution Company&apos; job / project.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Disallow_canceled_job_status_change</fullName>
        <active>true</active>
        <errorConditionFormula>AND(TEXT(PRIORVALUE(JobStatus__c)) = &apos;Canceled&apos;, TEXT(JobStatus__c) != &apos;Canceled&apos;)</errorConditionFormula>
        <errorMessage>Cannot update the status of a canceled job.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Disallow_keep_asng_for_bundled_projects</fullName>
        <active>true</active>
        <description>Keep Assignment cannot be different from location in case of bundled projects.</description>
        <errorConditionFormula>AND( Project__r.BundleServices__c, UPPER(TEXT(KeepAssignmentOverride__c)) !=  &apos;NO OVERRIDE&apos;, NOT ISBLANK(TEXT(KeepAssignmentOverride__c)))</errorConditionFormula>
        <errorMessage>Keep Assignment cannot be overridden for bundled projects.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Disallow_multi_days_for_bundled_projects</fullName>
        <active>true</active>
        <errorConditionFormula>AND( NumberOfDays__c &gt; 1,   Project__r.BundleServices__c )</errorConditionFormula>
        <errorDisplayField>NumberOfDays__c</errorDisplayField>
        <errorMessage>Number of days cannot be more than 1 for bundled projects.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Disallow_multi_workers_for_AMW_False</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NumberOfWorkers__c &gt; 1,  ProjectService__r.Service__r.IsNumberOfWorkersRequired__c == false)</errorConditionFormula>
        <errorDisplayField>NumberOfWorkers__c</errorDisplayField>
        <errorMessage>Number Of Workers cannot be more than 1 if the service&apos;s AllowMultipleWorkers is set to false.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Disallow_multi_workers_for_bundled_proje</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NumberOfWorkers__c &gt; 1,   Project__r.BundleServices__c )</errorConditionFormula>
        <errorDisplayField>NumberOfWorkers__c</errorDisplayField>
        <errorMessage>Number of workers cannot be more than 1 for bundled projects.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Disallow_past_schedule_dates</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISCHANGED( JobScheduledDate__c),JobScheduledDate__c &lt; TODAY())</errorConditionFormula>
        <errorMessage>Cannot schedule a job to a date that is in the past.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Disallow_schdate_change_diff_from_PLdate</fullName>
        <active>true</active>
        <description>Do not allow a scheduled date that is different from a project location scheduled date in case of a bundled project</description>
        <errorConditionFormula>AND(Project__r.BundleServices__c,ISCHANGED(JobScheduledDate__c), JobScheduledDate__c !=  ProjectLocation__r.LocationScheduledDate__c)</errorConditionFormula>
        <errorMessage>Job Scheduled date has to match the location scheduled date in case of bundled work.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Disallow_starttime_for_bundled_projects</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Project__r.BundleServices__c,NOT(ISBLANK(StartTime__c)))</errorConditionFormula>
        <errorDisplayField>JobStartTime__c</errorDisplayField>
        <errorMessage>Start Time cannot be set for Bundled project.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ECNoReStartReportedJob4InprogressProject</fullName>
        <active>true</active>
        <description>An execution company can&apos;t restart a reported job if the project is in progress.</description>
        <errorConditionFormula>AND(
Lower(ProjectScheduleOptionCode__c) == &apos;ec&apos;,
Lower(Text(JobStatus__c)) == &apos;started&apos;,
Lower(Text(PRIORVALUE(JobStatus__c))) == &apos;reported&apos;,
Lower(ProjectStatus__c) == &apos;in progress&apos;
)</errorConditionFormula>
        <errorMessage>An execution company can&apos;t restart a reported job if the project is in progress.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>JobDates_outside_project_window</fullName>
        <active>true</active>
        <errorConditionFormula>OR(StartDateCalculated__c &lt;  ProjectStartDate__c, StartDateCalculated__c &gt;  ProjectEndDate__c, EndDateCalculated__c  &lt; ProjectStartDate__c, EndDateCalculated__c  &gt; ProjectEndDate__c, JobScheduledDate__c &lt;  ProjectStartDate__c, JobScheduledDate__c &gt;  ProjectEndDate__c)</errorConditionFormula>
        <errorMessage>One or more job dates (Start/End/Scheduled) are outside project window.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>JobStartDateNotLaterThanEndDate</fullName>
        <active>true</active>
        <description>Job Start Date can not be later than Job End Date</description>
        <errorConditionFormula>JobStartDate__c &gt; JobEndDate__c</errorConditionFormula>
        <errorMessage>Job Start Date can not be later than Job End Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NoCancelReportedJob4EndedProject</fullName>
        <active>true</active>
        <description>CSM can&apos;t cancel a reported job if the project is ended.</description>
        <errorConditionFormula>AND(
LOWER(TEXT(PRIORVALUE(JobStatus__c))) == &apos;reported&apos;,
LOWER(TEXT(JobStatus__c)) == &apos;canceled&apos;,
LOWER(ProjectStatus__c) == &apos;ended&apos;
)</errorConditionFormula>
        <errorMessage>CSM can&apos;t cancel a reported job if the project is ended.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NoReschedule4ReportedJobWithEndedProject</fullName>
        <active>true</active>
        <description>CSM can&apos;t reschedule a reported job if the project is ended.</description>
        <errorConditionFormula>AND(
ISCHANGED(JobScheduledDate__c),
LOWER(TEXT(PRIORVALUE(JobStatus__c))) == &apos;reported&apos;,
LOWER(ProjectStatus__c) == &apos;ended&apos;
)</errorConditionFormula>
        <errorMessage>CSM can&apos;t reschedule a reported job if the project is ended.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NoUpdateOnReportedJob4CanclClsProject</fullName>
        <active>true</active>
        <description>A reported job cannot be updated when the project is either closed or canceled.</description>
        <errorConditionFormula>AND(
Lower(TEXT(PRIORVALUE(JobStatus__c))) == &apos;reported&apos;,
OR(
Lower(ProjectStatus__c) == &apos;closed&apos;,
Lower(ProjectStatus__c) == &apos;canceled&apos;
)
)</errorConditionFormula>
        <errorMessage>A reported job cannot be updated when the project is either closed or canceled.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NotReschduleReportedJobWithEndedProject</fullName>
        <active>true</active>
        <description>CSM can&apos;t reschedule a &apos;Reported&apos; job if the Project status is &apos;Ended&apos;</description>
        <errorConditionFormula>AND ( 
LOWER(ProjectStatus__c) == &apos;ended&apos;,
ISPICKVAL(JobStatus__c, &apos;Reported&apos;),
PRIORVALUE(JobScheduledDate__c) &lt;&gt; null,
PRIORVALUE(JobScheduledDate__c) &lt;&gt; JobScheduledDate__c 
)</errorConditionFormula>
        <errorMessage>A reported job can not be rescheduled if the project is ended.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NotRestartReportedJobForInProgressProjec</fullName>
        <active>true</active>
        <description>EC can not restart a reported job if the project is in progress.</description>
        <errorConditionFormula>AND(
ISPICKVAL(JobStatus__c, &apos;Started&apos;),
ISPICKVAL(JobStatus__c, &apos;Reported&apos;),
LOWER(ProjectStatus__c) == &apos;in progress&apos;,
LOWER(ProjectScheduleOptionCode__c) == &apos;ec&apos;
)</errorConditionFormula>
        <errorMessage>EC can not restart a reported job if the project is in progress.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Allowed_Reschedule_Dates</fullName>
        <active>true</active>
        <description>Check if the new reschedule date is in the list of allowed reschedule dates.</description>
        <errorConditionFormula>AND(CONTAINS($User.Username,&quot;salesforce.integration&quot;), ISCHANGED(JobScheduledDate__c),!ISBLANK(Project__r.AllowedRescheduleDates__c), !ISNULL(Project__r.AllowedRescheduleDates__c), !CONTAINS(Project__r.AllowedRescheduleDates__c, TEXT(JobScheduledDate__c)))</errorConditionFormula>
        <errorMessage>New reschedule date not in the list of allowed schedule dates.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Start_Time</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(
OR(
ISBLANK(JobStartTime__c),
REGEX(JobStartTime__c, &quot;^(1[0-2]|0?[1-9]):[0-5][0-9] ([AaPp][Mm])$&quot;)
)
)</errorConditionFormula>
        <errorDisplayField>JobStartTime__c</errorDisplayField>
        <errorMessage>Invalid Start Time. Please follow this format HH:MM AMPM</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>prevent_negative_values_Number_of_Worker</fullName>
        <active>true</active>
        <errorConditionFormula>NumberOfWorkers__c &lt;0</errorConditionFormula>
        <errorMessage>Number Of Workers Field  not accept  zero and negative numbers.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>prevent_negative_values_purchase_amount</fullName>
        <active>true</active>
        <errorConditionFormula>PurchaseAmount__c &lt;0</errorConditionFormula>
        <errorMessage>Purchase Amount  Field not accept zero and negative numbers</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>prevent_negative_valuesestimated_minutes</fullName>
        <active>true</active>
        <errorConditionFormula>EstimatedMinutesOverride__c &lt;0</errorConditionFormula>
        <errorMessage>Estimated Minutes  Field not accept zero and negative numbers</errorMessage>
    </validationRules>

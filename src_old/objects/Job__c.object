    <fields>
        <fullName>IsCancellable__c</fullName>
        <description>The checkbox that indicates if this Job can be canceled.</description>
        <externalId>false</externalId>
        <formula>OR 
(

JobCancelOverride__c == true, 

AND(

	AND(LOWER(ProjectStatus__c) != &apos;planning&apos;, LOWER(ProjectStatus__c) != &apos;closed&apos;, LOWER(ProjectStatus__c) != &apos;pending&apos;), 
	LOWER(TEXT(JobStatus__c)) != &apos;canceled&apos;, 
	LOWER(TEXT(JobStatus__c)) != &apos;closed&apos;, 

(DATETIMEVALUE(NULLVALUE(JobScheduledDate__c, JobStartDate__c)) - NOW() - 
													((NULLVALUE(JobCancelLeadTime__c,0) - 5)/24)) &gt; 0

))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Job can be canceled if Project Status = Booked, Launched or In Progress and Job is within Cancel Lead Time.</inlineHelpText>
        <label>Cancelable</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>IsCompletedinWindow__c</fullName>

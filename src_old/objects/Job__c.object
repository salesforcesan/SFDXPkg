        <externalId>false</externalId>
        <formula>AND(( JobCompletedDate__c &gt;=OriginalProjectStartDate__c),( JobCompletedDate__c &lt;=Project__r.LaunchedProjectEndDate__c))&amp;&amp; Text(CompletionStatus__c)=&quot;Completed&quot;</formula>

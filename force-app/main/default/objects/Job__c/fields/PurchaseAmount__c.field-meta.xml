<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PurchaseAmount__c</fullName>
    <description>The field that indicates the Purchase Amount for the Job.</description>
    <externalId>false</externalId>
    <formula>IF(ISBLANK( PurchaseOverRideAmount__c), ProjectService__r.PurchaseAmount__c,PurchaseOverRideAmount__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>The purchase amount for the job</inlineHelpText>
    <label>Purchase Amount</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>

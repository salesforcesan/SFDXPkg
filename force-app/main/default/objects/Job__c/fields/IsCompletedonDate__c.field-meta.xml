<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>IsCompletedonDate__c</fullName>
    <description>The checkbox that indicates if the Job was completed on date.</description>
    <externalId>false</externalId>
    <formula>Text(CompletionStatus__c) = &quot;Completed&quot; &amp;&amp; JobCompletedDate__c== JobCommittedDate__c</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>If selected, the job was completed on date.</inlineHelpText>
    <label>Completed on Date</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>

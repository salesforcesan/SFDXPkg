<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>DaysSinceApprovalRequestCount__c</fullName>
    <description>A field that indicates the number of days since the Project was approved..</description>
    <externalId>false</externalId>
    <formula>DATEVALUE(ApprovalRequestDate__c) -  TODAY()</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>The number of days since the project was approved</inlineHelpText>
    <label># Days Since Approval Request Count</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>

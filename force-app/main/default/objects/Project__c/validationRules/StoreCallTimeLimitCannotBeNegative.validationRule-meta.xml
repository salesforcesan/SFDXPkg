<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>StoreCallTimeLimitCannotBeNegative</fullName>
    <active>true</active>
    <errorConditionFormula>StoreCallTimeLimit__c &lt; 0</errorConditionFormula>
    <errorMessage>Store Call Time Minutes must be 0 or greater</errorMessage>
</ValidationRule>

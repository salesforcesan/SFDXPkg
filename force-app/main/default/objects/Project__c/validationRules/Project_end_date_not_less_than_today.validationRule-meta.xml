<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Project_end_date_not_less_than_today</fullName>
    <active>true</active>
    <errorConditionFormula>AND(EndDate__c &lt; today(),OR(ISNEW(),ISCHANGED(EndDate__c)))</errorConditionFormula>
    <errorMessage>Start date cannot come after the end date</errorMessage>
</ValidationRule>

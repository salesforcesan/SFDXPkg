<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Project_start_date_not_less_than_today</fullName>
    <active>true</active>
    <errorConditionFormula>AND(StartDate__c &lt; today(),OR(ISNEW(),ISCHANGED(StartDate__c)))</errorConditionFormula>
    <errorDisplayField>StartDate__c</errorDisplayField>
    <errorMessage>Project start  date cannot be less than today</errorMessage>
</ValidationRule>

<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Service_Title_is_required</fullName>
    <active>true</active>
    <errorConditionFormula>OR(ISBLANK(ServiceTitle__c), ISNULL(ServiceTitle__c))</errorConditionFormula>
    <errorDisplayField>ServiceTitle__c</errorDisplayField>
    <errorMessage>Service Title cannot be empty.</errorMessage>
</ValidationRule>

<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cannot_Change_Asgnmnt_for_Started_Worker</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISCHANGED(ContactId__c),IsCheckedIn__c == True)</errorConditionFormula>
    <errorMessage>Cannot change assignment of a worker who has already checked in and started the work.</errorMessage>
</ValidationRule>
